<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          https://maven.apache.org/xsd/settings-1.0.0.xsd">
	<localRepository/>
	<interactiveMode/>
	<offline/>
	<pluginGroups/>
	<servers>
		<server>
			<id>mule-ee-repository</id>
			<username>${env.nexusEEClientId}</username>
			<password>${env.nexusEEClientSecret}</password>
		</server>
				
		<server>
			<id>Exchange-v3</id>
			<username>~~~Client~~~</username>
			<password>${env.mavenExchangeClientId}~?~${env.mavenExchangeClientSecret}</password>
		</server>
		
		<server>
			<id>anypoint-exchange-v3</id>
			<username>~~~Client~~~</username>
			<password>${env.mavenExchangeClientId}~?~${env.mavenExchangeClientSecret}</password>
		</server>
		
		<server>
			<id>mule-exchange</id>
			<username>~~~Client~~~</username>
			<password>${env.mavenExchangeClientId}~?~${env.mavenExchangeClientSecret}</password>
		</server>
		
		<server>
			<id>ExchangeV2</id>
			<username>~~~Client~~~</username>
			<password>${env.mavenExchangeClientId}~?~${env.mavenExchangeClientSecret}</password>
		</server>
		
		<server>
			<id>github</id>
			<username>${env.GITHUB_ACTOR}</username>
			<password>${env.GITHUB_TOKEN}</password>
		</server>

	</servers>
	<mirrors/>
	<proxies/>
	<profiles/>
	<activeProfiles/>
</settings>
